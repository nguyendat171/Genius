{
  "name": "genius",
  "description": "Genius web application",
  "version": "1.0.0",
  "engines": {
    "npm": ">=6.12.0",
    "node": ">=12.13.0"
  },
  "keywords": [],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/reportWebVitals.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.4.0",
    "@types/bcrypt": "5.0.0",
    "@types/react-router-redux": "5.0.21",
    "@types/reactstrap": "8.7.2",
    "animate.css": "4.1.1",
    "axios": "0.21.1",
    "bcrypt": "5.0.1",
    "bootstrap": "5.1.3",
    "classnames": "2.3.1",
    "formik": "2.2.9",
    "node-sass": "^4.12.0",
    "numeral": "2.0.6",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.1",
    "react-router-dom": "5.2.0",
    "react-router-redux": "4.0.8",
    "react-toastify": "7.0.4",
    "reactstrap": "9.0.1",
    "redux-injectors": "1.3.0",
    "redux-saga": "1.1.3",
    "sanitize.css": "12.0.1",
    "web-vitals": "0.2.4",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.3",
    "@types/jest": "25.1.4",
    "@types/numeral": "2.0.1",
    "@types/react": "17.0.0",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/react-router-dom": "5.1.5",
    "@types/react-test-renderer": "16.9.3",
    "@types/testing-library__jest-dom": "5.9.2",
    "@types/webpack": "4.41.22",
    "@types/webpack-env": "1.15.2",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react-hooks": "4.1.0",
    "lint-staged": "10.3.0",
    "prettier": "2.1.1",
    "react-scripts": "4.0.1",
    "typescript": "4.1.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
